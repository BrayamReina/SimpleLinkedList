package structures;

public class SimpleLinkedList<T> {
    private Node<T> head;

    public SimpleLinkedList() {
        this.head = null;
    }

    public void add(E e) {
        Node<T> newNode = new Node<>(e);
        if (head == null) {
            head = newNode;
        } else {
            Node<T> current = head;
            while (current.next != null) {
                current = current.next;
            }
            current.next = newNode;
        }
    }

  //Not implemented 
  public boolean add(int index, E element){
      return false;
  }

